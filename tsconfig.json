{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@assets/*": ["src/assets/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "strictNullChecks": false,
    "allowJs": true,
    "lib": ["DOM", "ESNext"]
  },
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"]
}
